{
  "expo": {
    "entryPoint": "./frontend/App.tsx",
    "name": "web",
    "slug": "web",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/icon.png",
    "splash": {
      "image": "./frontend/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./frontend/assets/favicon.png"
    }
  }
}
